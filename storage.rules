rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Legacy support
    match /outfits/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
    // Studio-based isolation
    match /studios/{studioId}/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
